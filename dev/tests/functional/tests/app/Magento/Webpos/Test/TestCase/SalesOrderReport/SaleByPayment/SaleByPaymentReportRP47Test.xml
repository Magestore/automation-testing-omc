<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/etc/variations.xsd">
    <testCase name="Magento\Webpos\Test\TestCase\SalesOrderReport\SaleByPayment\SaleByPaymentReportRP47Test"
              summary="Order List By Payment Report" ticketId="MAGETWO-28222">
        <variation name="SaleByPaymentReportRP46TestVariation">
            <data name="shifts/from" xsi:type="string">m/d/Y -1 month</data>
            <data name="shifts/to" xsi:type="string">m/d/Y</data>
            <data name="shifts/show_order_statuses" xsi:type="string">Any</data>
        </variation>
        <variation name="SaleByPaymentReportRP48TestVariation">
            <data name="shifts/show_order_statuses" xsi:type="string">Specified</data>
            <data name="order_status" xsi:type="string">Canceled, Closed, Complete,Suspected Fraud, On Hold, Payment
                Review, PayPal Canceled Reversal, PayPal Reversed, Pending, Pending, Pending PayPal, Processing
            </data>
        </variation>
        <variation name="SaleByPaymentReportRP49TestVariation">
            <data name="shifts/from" xsi:type="string">m/d/Y -1 month</data>
            <data name="shifts/to" xsi:type="string">m/d/Y</data>
            <data name="shifts/show_order_statuses" xsi:type="string">Specified</data>
            <data name="order_status" xsi:type="string">Canceled, Closed, Complete,Suspected Fraud, On Hold, Payment
                Review, PayPal Canceled Reversal, PayPal Reversed, Pending, Pending, Pending PayPal, Processing
            </data>
        </variation>
    </testCase>
</config>
