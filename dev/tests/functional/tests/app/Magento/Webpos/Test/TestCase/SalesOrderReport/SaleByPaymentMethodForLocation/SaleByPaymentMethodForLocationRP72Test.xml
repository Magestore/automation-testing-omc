            name="Magento\Webpos\Test\TestCase\SalesOrderReport\SaleByPaymentMethodForLocation\SaleByPaymentMethodForLocationRP72Test"
            summary="Sales By Staff Daily Report" ticketId="MAGETWO-28222">
