<?xml version="1.0" encoding="utf-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/etc/variations.xsd">
    <testCase name="Magento\Webpos\Test\TestCase\Zreport\WebposXreportCheckGUITest"
              summary="Grid UI Component Filtering" ticketId="WEBPOS-1">
        <variation name="WebposXreportCheckGUIZR021Test">
            <data name="products" xsi:type="array">
                <item name="0" xsi:type="string">product1</item>
            </data>
            <constraint name="Magento\Webpos\Test\Constraint\Zreport\AssertGUIXreport"/>
        </variation>
        <variation name="WebposXreportCheckGUIZR022Test">
            <data name="products" xsi:type="array">
                <item name="0" xsi:type="string">product1</item>
            </data>
            <data name="posName" xsi:type="string">Store POS</data>
            <constraint name="Magento\Webpos\Test\Constraint\Zreport\AssertGUIXZreportPosName"/>
        </variation>
        <variation name="WebposXreportCheckGUIZR023Test">
            <data name="products" xsi:type="array">
                <item name="0" xsi:type="string">product1</item>
            </data>
            <constraint name="Magento\Webpos\Test\Constraint\Zreport\AssertGUIXZreportStaffName"/>
        </variation>
        <variation name="WebposXreportCheckGUIZR025Test">
            <data name="products" xsi:type="array">
                <item name="0" xsi:type="string">product1</item>
            </data>
            <constraint name="Magento\Webpos\Test\Constraint\Zreport\AssertGUIXreportOpened"/>
        </variation>
    </testCase>
</config>
