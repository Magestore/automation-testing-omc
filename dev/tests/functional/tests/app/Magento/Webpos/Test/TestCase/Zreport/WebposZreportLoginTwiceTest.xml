<?xml version="1.0" encoding="utf-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/etc/variations.xsd">
    <testCase name="Magento\Webpos\Test\TestCase\Zreport\WebposZreportLoginTwiceTest"
              summary="Grid UI Component Filtering" ticketId="WEBPOS-1">
        <variation name="WebposZreportZR004Test">
            <data name="products" xsi:type="array">
                <item name="0" xsi:type="string">product1</item>
            </data>
            <data name="pos/data/pos_name" xsi:type="string">test pos %isolation%</data>
            <data name="pos/data/status" xsi:type="string">Enabled</data>
            <data name="pos/data/is_allow_to_lock" xsi:type="string">0</data>
            <data name="denomination/dataset" xsi:type="string">default</data>
            <data name="denominationNumberCoin" xsi:type="string">1</data>
            <constraint name="Magento\Webpos\Test\Constraint\Zreport\AssertGUIXZreportStaffName"/>
        </variation>
        <variation name="WebposZreportZR007Test">
            <data name="products" xsi:type="array">
                <item name="0" xsi:type="string">product1</item>
            </data>
            <data name="pos/data/pos_name" xsi:type="string">test pos %isolation%</data>
            <data name="pos/data/status" xsi:type="string">Enabled</data>
            <data name="pos/data/is_allow_to_lock" xsi:type="string">0</data>
            <data name="denomination/dataset" xsi:type="string">default</data>
            <data name="denominationNumberCoin" xsi:type="string">1</data>
            <constraint name="Magento\Webpos\Test\Constraint\Zreport\AssertGUIZreportOpenedClosed"/>
        </variation>
    </testCase>
</config>
