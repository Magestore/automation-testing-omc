<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Magento\Customer\Test\Repository\Address">
        <dataset name="default_webpos_guest_address">
            <field name="firstname" xsi:type="string">Guest</field>
            <field name="lastname" xsi:type="string">POS</field>
            <field name="email" xsi:type="string">guest@example.com</field>
            <field name="street" xsi:type="string">Street</field>
            <field name="city" xsi:type="string">Guest City</field>
            <field name="region_id" xsi:type="string">California</field>
            <field name="postcode" xsi:type="string">90034</field>
            <field name="country_id" xsi:type="string">United States</field>
            <field name="telephone" xsi:type="string">12345678</field>
        </dataset>
    </repository>
</config>
