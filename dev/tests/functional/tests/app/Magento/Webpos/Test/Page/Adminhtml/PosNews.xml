<?xml version="1.0" encoding="utf-8"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../vendor/magento/mtf/etc/pages.xsd">
    <page name="PosNews" area="Adminhtml" mca="webposadmin/pos/new" module="Magento_Webpos">
        <!-- For identifying a form block on the HTML page -->
        <block name="posForm" class="Magento\Webpos\Test\Block\Adminhtml\Pos\Edit\PosForm" locator="[id='page:main-container']" strategy="css selector" />
        <!-- this code for save action -->
        <block name="formPageActions" class="Magento\Webpos\Test\Block\Adminhtml\Pos\Edit\FormPageActions" locator=".page-main-actions" strategy="css selector" />
        <block name="messagesBlock" class="Magento\Backend\Test\Block\Messages" locator="#messages .messages" strategy="css selector"/>
        <block name="modalsWrapper" class="Magento\Webpos\Test\Block\Adminhtml\ModalsWrapper" locator=".modals-wrapper" strategy="css selector"/>
    </page>
</config>
