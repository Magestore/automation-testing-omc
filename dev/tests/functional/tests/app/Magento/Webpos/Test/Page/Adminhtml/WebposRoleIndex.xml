<?xml version="1.0" encoding="utf-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../vendor/magento/mtf/etc/pages.xsd">
    <page name="WebposRoleIndex" area="Adminhtml" mca="webposadmin/staff_role/index" module="Magento_Webpos">
        <block name="pageActionsBlock" class="Magento\Backend\Test\Block\GridPageActions" locator=".page-main-actions" strategy="css selector"/>
        <block name="messagesBlock" class="Magento\Backend\Test\Block\Messages" locator="#messages .messages" strategy="css selector"/>
        <block name="gridPageActions" class="Magento\Backend\Test\Block\GridPageActions" locator=".page-main-actions" strategy="css selector" />
        <block name="roleGrid" class="Magento\Webpos\Test\Block\Adminhtml\Role\RoleGrid" locator="[id='page:main-container']" strategy="css selector" />
        <block name="editableRow" class="Magento\Webpos\Test\Block\Adminhtml\Role\EditableRow" locator="//tr[@class='data-grid-editable-row']" strategy="xpath" />
        <block name="titleBlock" class="Magento\Theme\Test\Block\Html\Title" locator=".page-title-wrapper" strategy="css selector"></block>
        <block name="modal" class="Magento\Webpos\Test\Block\Modal" locator=".modals-wrapper" strategy="css selector"/>
    </page>
</config>
