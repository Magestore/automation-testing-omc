			<constraint name="Magento\Webpos\Test\Constraint\Settings\Login\AssertLoginFailed" />
		</variation>
		<variation name="WebposSettingLoginTestVariation3">
			<data name="staff/data/username" xsi:type="string">magentoadmin</data>
			<data name="staff/data/password" xsi:type="string">admin123</data>
			<constraint name="Magento\Webpos\Test\Constraint\Settings\Login\AssertLoginSuccess" />
		</variation>
	</testCase>
