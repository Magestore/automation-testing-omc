<?xml version="1.0" encoding="utf-8"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../vendor/magento/mtf/etc/pages.xsd">
    <page name="DenominationNews" area="Adminhtml" mca="webposadmin/denomination/new" module="Magento_Webpos">
        <!-- For identifying a form block on the HTML page -->
        <block name="denominationsForm" class="Magento\Webpos\Test\Block\Adminhtml\Denomination\Edit\DenominationForm" locator="[id='page:main-container']" strategy="css selector" />
        <!-- this code for save action -->
        <block name="formPageActions" class="Magento\Backend\Test\Block\FormPageActions" locator=".page-main-actions" strategy="css selector" />

        <block name="messagesBlock" class="Magento\Backend\Test\Block\Messages" locator="#messages .messages" strategy="css selector"/>

        <block name="Container" class="Magento\Webpos\Test\Block\Adminhtml\Container" locator="#container" strategy="css selector" />
        <block name="PageWrapper" class="Magento\Webpos\Test\Block\Adminhtml\PageWrapper" locator=".page-wrapper" strategy="css selector" />
    </page>
</config>
