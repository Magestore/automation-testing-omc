<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/etc/variations.xsd">
    <testCase name="Magento\Webpos\Test\TestCase\SalesOrderReport\SalesByPaymentEntityTest"
              summary="Sales By Payment Report" ticketId="MAGETWO-28222">
        <variation name="SalesByPaymentEntityTestVariation1">
            <data name="shifts/from" xsi:type="string">m/d/Y -1 year</data>
            <data name="shifts/to" xsi:type="string">m/d/Y</data>
            <data name="shifts/show_order_statuses" xsi:type="string">Any</data>
        </variation>
        <variation name="SalesByPaymentEntityTestVariation2">
            <data name="shifts/from" xsi:type="string">m/d/Y -1 month</data>
            <data name="shifts/to" xsi:type="string">m/d/Y</data>
            <data name="shifts/show_order_statuses" xsi:type="string">Any</data>
        </variation>
        <variation name="SalesByPaymentEntityTestVariation3">
            <data name="shifts/from" xsi:type="string">m/d/Y -1 day</data>
            <data name="shifts/to" xsi:type="string">m/d/Y +1 day</data>
            <data name="shifts/show_order_statuses" xsi:type="string">Any</data>
        </variation>
    </testCase>
</config>
