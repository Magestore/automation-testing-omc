<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Magento\Webpos\Test\Repository\Denomination">
        <dataset name="default">
            <field name="denomination_name" xsi:type="string">Test Denomination Name %isolation%</field>
            <field name="denomination_value" xsi:type="string">10</field>
            <field name="sort_order" xsi:type="string">%isolation%</field>
        </dataset>

        <dataset name="default_null">
            <field name="denomination_name" xsi:type="string" />
            <field name="denomination_value" xsi:type="string" />
            <field name="sort_order" xsi:type="string" />
        </dataset>

        <dataset name="one_dollar">
            <field name="denomination_name" xsi:type="string">Denomination One Dollar %isolation%</field>
            <field name="denomination_value" xsi:type="string">1</field>
            <field name="sort_order" xsi:type="string">%isolation%</field>
        </dataset>

        <dataset name="ten_dollar">
            <field name="denomination_name" xsi:type="string">Denomination Ten Dollar %isolation%</field>
            <field name="denomination_value" xsi:type="string">10</field>
            <field name="sort_order" xsi:type="string">%isolation%</field>
        </dataset>
    </repository>
</config>
