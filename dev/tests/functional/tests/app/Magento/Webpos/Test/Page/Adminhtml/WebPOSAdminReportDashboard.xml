        <block name="staffReport" class="Magento\Webpos\Test\Block\Adminhtml\Report\StaffReport" locator=".admin-report-webpos > div > div:nth-child(1)" strategy="css selector" />
        <block name="locationReport" class="Magento\Webpos\Test\Block\Adminhtml\Report\LocationReport" locator=".admin-report-webpos > div > div:nth-child(2)" strategy="css selector" />
