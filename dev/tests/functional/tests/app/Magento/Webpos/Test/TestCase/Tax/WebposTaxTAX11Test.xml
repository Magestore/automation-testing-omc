<?xml version="1.0" encoding="utf-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../vendor/magento/mtf/etc/variations.xsd">
	<testCase name="Magento\Webpos\Test\TestCase\Tax\WebposTaxTAX11Test" summary="Tax - Check tax amount when ordering without shipping fee and discount amount test" ticketId="WEBPOS-1">
		<variation name="WebposTaxTAX11TestVariation_TAX11">
			<data name="configData" xsi:type="string">default_tax_configuration_use_system_value</data>
			<data name="products" xsi:type="array">
				<item name="0" xsi:type="array">
					<item name="product" xsi:type="string">product_100_dollar_taxable</item>
					<item name="orderQty" xsi:type="number">2</item>
				</item>
				<item name="1" xsi:type="array">
					<item name="product" xsi:type="string">product_100_dollar_taxable</item>
					<item name="orderQty" xsi:type="number">2</item>
				</item>
			</data>
			<data name="createInvoice" xsi:type="boolean">true</data>
			<data name="shipped" xsi:type="boolean">true</data>
			<data name="expectStatus" xsi:type="string">Closed</data>

			<constraint name="Magento\Webpos\Test\Constraint\OrderHistory\Refund\AssertRefundSuccess"/>
			<constraint name="Magento\Webpos\Test\Constraint\Tax\AssertTaxOnOrderDetailIsCorrect"/>
		</variation>
	</testCase>
</config>