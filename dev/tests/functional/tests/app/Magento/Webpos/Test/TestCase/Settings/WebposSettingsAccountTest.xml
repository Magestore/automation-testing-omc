<?xml version="1.0" encoding="utf-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../vendor/magento/mtf/etc/variations.xsd">
	<testCase name="Magento\Webpos\Test\TestCase\Settings\WebposSettingsAccountTest" summary="Frontend Setting Account Test" ticketId="WEBPOS-1">
		<variation name="WebposSettingsAccountTestVariation1">
			<data name="staff/data/username" xsi:type="string">abc</data>
			<data name="staff/data/password" xsi:type="string">abc123456</data>
			<constraint name="Magento\Webpos\Test\Constraint\Settings\Account\AssertBlankSpace" />
		</variation>
		<variation name="WebposSettingsAccountTestVariation2">
			<data name="staff/data/username" xsi:type="string">abc</data>
			<data name="staff/data/password" xsi:type="string">abc123456</data>
			<data name="displayName" xsi:type="string">abc %isolation%</data>
			<data name="currentPassword" xsi:type="string">abc123456</data>
			<constraint name="Magento\Webpos\Test\Constraint\Settings\Account\AssertEditDisplayName" />
		</variation>
		<variation name="WebposSettingsAccountTestVariation3">
			<data name="staff/data/username" xsi:type="string">abc</data>
			<data name="staff/data/password" xsi:type="string">abc123456</data>
			<data name="displayName" xsi:type="null"></data>
			<data name="newPassword" xsi:type="string">abc%isolation%</data>
			<constraint name="Magento\Webpos\Test\Constraint\Settings\Account\AssertVariation3" />
		</variation>
		<variation name="WebposSettingsAccountTestVariation4">
			<data name="staff/data/username" xsi:type="string">abc</data>
			<data name="staff/data/password" xsi:type="string">abc123456</data>
			<data name="displayName" xsi:type="null"></data>
			<data name="currentPassword" xsi:type="string">abc</data>
			<constraint name="Magento\Webpos\Test\Constraint\Settings\Account\AssertVariation4" />
		</variation>
		<variation name="WebposSettingsAccountTestVariation5">
			<data name="staff/data/username" xsi:type="string">abc</data>
			<data name="staff/data/password" xsi:type="string">abc123456</data>
			<data name="displayName" xsi:type="null"></data>
			<data name="currentPassword" xsi:type="string">abc123456</data>
			<data name="newPassword" xsi:type="string">abc</data>
			<constraint name="Magento\Webpos\Test\Constraint\Settings\Account\AssertVariation5" />
		</variation>
		<variation name="WebposSettingsAccountTestVariation6">
			<data name="staff/data/username" xsi:type="string">abc</data>
			<data name="staff/data/password" xsi:type="string">abc123456</data>
			<data name="displayName" xsi:type="null"></data>
			<data name="currentPassword" xsi:type="string">abc123456</data>
			<data name="newPassword" xsi:type="string">a12345678</data>
			<data name="samePass" xsi:type="boolean">true</data>
			<data name="tearDown" xsi:type="boolean">true</data>

			<constraint name="Magento\Webpos\Test\Constraint\Settings\Account\AssertVariation6" />
		</variation>
	</testCase>
</config>